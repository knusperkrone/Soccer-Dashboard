<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/playfieldlayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_vertical"
    android:orientation="horizontal"
    tools:context=".MainActivity">

    <ca.hss.heatmaplib.HeatMap
        android:id="@+id/heatmap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:maxOpacity="255"
        app:minOpacity="0"
        app:radius="300dp"/>

    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:layoutDirection="rtl"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/goalkeeper_paddingsize_large">

            <TableLayout
                android:layout_width="@dimen/overviewplayer_size_large"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p1"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_gravity="center_vertical"
                    android:layout_margin="5dp"/>

            </TableLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/lines4_line_paddingsize_large">

            <TableLayout
                android:layout_width="@dimen/overviewplayer_size_large"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p6"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p5"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p2"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p3"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

            </TableLayout>

        </FrameLayout>


        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/lines4_line_paddingsize_large">

            <TableLayout
                android:layout_width="@dimen/overviewplayer_size_large"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p7"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p4"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p8"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p11"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

            </TableLayout>

        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:paddingStart="@dimen/lines4_line_paddingsize_large">

            <TableLayout
                android:layout_width="@dimen/overviewplayer_size_large"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:gravity="center_vertical">

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p10"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

                <de.uni_erlangen.wi1.footballdashboard.ui_components.fragment_overview.custom_views.PlayerView
                    android:id="@+id/p9"
                    android:layout_width="@dimen/overviewplayer_size_large"
                    android:layout_height="@dimen/overviewplayer_size_large"
                    android:layout_margin="5dp"
                    android:layout_weight=".20"/>

            </TableLayout>

        </FrameLayout>

    </LinearLayout>

</FrameLayout>